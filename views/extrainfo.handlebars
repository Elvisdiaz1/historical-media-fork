<div class="container" id="main">
  <div class="card">
    <div class="card-header" id="year">
      {{event.year}}
    </div>
    <div class="card-body">
      <p class="card-text">{{event.description}}</p>
    </div>
  </div> 

    {{!-- This is for the wiki links --}}
    
  <div class="list-group">
    <div class="container" id="main">
  
     {{#each event.wikipedia as |info|}}
      {{> extrainfopartial info}}
        {{/each}}
    </div>

  </div>


    {{!-- This is for the blogs about htis event --}}
    <div class="list-group">
      <a
        {{!-- href="#" --}}
        class="list-group-item list-group-item-action"
        aria-current="true"
      >
      <div class="container" id="main">
  {{#each event.blogs as |info|}}
    {{> wikipartials info}}
    {{/each}}
</div>
        
      </a>

    </div>
  </div>
</div>