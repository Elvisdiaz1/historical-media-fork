<div class="container" id="dashcontainer" >
    <h3 id="author">
    Welcome {{userInfo.username}}
    </h3> 
    {{#if userBlogs}}
    <div class="container">
        {{#each userBlogs as |event|}}
            {{> parti-dash event}}
        {{/each}}
    </div>
    {{else}}
        <p id="no-blogs">No Blogs Published</p>
    {{/if}}
</div>